<?xml version="1.0" encoding="utf-8"?>
<com.anshi.lazyshopmall.self_view.CallBackScrollView xmlns:android="http://schemas.android.com/apk/res/android"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent"

    >
    <LinearLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:orientation="vertical"
        >
        <com.anshi.lazyshopmall.self_view.CanScrollGridView
            android:layout_marginBottom="@dimen/dimen_10_dip"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:id="@+id/main_grid"
            android:numColumns="3"
            android:listSelector="@android:color/transparent"
            android:verticalSpacing="20dp"
            />
    </LinearLayout>
</com.anshi.lazyshopmall.self_view.CallBackScrollView>